      id: "bdw-banner",
      localStorage: "bdw-topC",
    },
  ];
  bdwInitialDivs.map((item) => {
    if (
      document.getElementById(item.id) &&
      localStorage.getItem(item.localStorage)
    ) {
      bdwGondolasAmount++;
    }
  });
  let bdwGondolas = [
    {
      id: "braindw-mi-mundo-home-categorias-navegadas",
      loaded: false,
      message: "categorias-navegadas-loaded",
      height: bdwIsMobile ? "750px" : "650px",
