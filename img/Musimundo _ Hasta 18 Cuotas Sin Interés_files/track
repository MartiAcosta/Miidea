  $(".siteSearchInput").on("keyup", function (e) {
    var screenWidth = $(window).width() < 750 ? true : false;
    if (
      $(".siteSearchInput").last().val().length > 2 ||
      $(".siteSearchInput").first().val().length > 2
    ) {
      if (!screenWidth) {
        searchTerm = $(".siteSearchInput").first().val();
        bdwSearch(searchTerm);
      } else {
        searchTerm = $(".siteSearchInput").last().val();
        bdwSearch(searchTerm);
      }
    } else {
      $("#ui-id-1").remove();
      $("#ui-id-2").remove();
    }
  });

