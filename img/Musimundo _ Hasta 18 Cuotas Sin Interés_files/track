  "message",
  function (e) {
    if (!document.getElementById("braindw-mi-musimundo")) return null;
    var scroll_height = e.data;
    document.getElementById("braindw-mi-musimundo").style.height =
      scroll_height + "px";
  },
  false
);

window.onmessage(() => {
  if (!document.getElementById("braindw-mi-musimundo")) return null;
  var scroll_height = e.data;
  document.getElementById("braindw-mi-musimundo").style.height =
    scroll_height + "px";
});

function bewGetTopC() {
  var topCategorias = [];
  var braindwCategorias = [];
  if (localStorage.getItem("bdw-topC") != null) {
    topCategorias = JSON.parse(localStorage.getItem("bdw-topC"));
  }
  if (localStorage.getItem("bdw-categorias") != null) {
    braindwCategorias = JSON.parse(localStorage.getItem("bdw-categorias"));
  }
  var url = window.location.pathname.split("/");
  var categoryLength = $("#breadcrumb .container ul li a").length;

  $("#breadcrumb .container ul li a").each(function (index, element) {
    var currCat = $(this).attr("href");
    var code = currCat.split("/").pop();
    var href = currCat.split("/");
    var urlLetter = url[url.length - 2];

    if (urlLetter == "c" && index == categoryLength - 1) {
      localStorage.setItem("bdw-lastC", code);
      var created = false;
      if (braindwCategorias.length > 0) {
        for (var c = 0; c < braindwCategorias.length; c++) {
          if (!created) {
            if (braindwCategorias[c].c == code) {
              braindwCategorias[c].v = parseInt(braindwCategorias[c].v) + 1;
              var auxiliar = braindwCategorias;
              created = true;
              localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
            }
            if (
              c == braindwCategorias.length - 1 &&
              braindwCategorias[c].c != code &&
              !created
            ) {
              var auxiliar = braindwCategorias;
              auxiliar.push({ c: code, v: 1 });
              localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
              created = true;
            }
          }
        }
      } else {
        var auxiliar = [];
        auxiliar[0] = { c: code, v: 1 };
        localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
      }
    } else if (urlLetter == "p" && index == categoryLength - 2) {
      localStorage.setItem("bdw-lastC", code);
      var created = false;
      if (braindwCategorias.length > 0) {
        for (var c = 0; c < braindwCategorias.length; c++) {
          if (!created) {
            if (braindwCategorias[c].c == code) {
              braindwCategorias[c].v = parseInt(braindwCategorias[c].v) + 1;
              var auxiliar = braindwCategorias;
              created = true;
              localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
            }
            if (
              c == braindwCategorias.length - 1 &&
              braindwCategorias[c].c != code &&
              !created
            ) {
              var auxiliar = braindwCategorias;
              auxiliar.push({ c: code, v: 1 });
              localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
              created = true;
            }
          }
        }
      } else {
        var auxiliar = [];
        auxiliar[0] = { c: code, v: 1 };
        localStorage.setItem("bdw-categorias", JSON.stringify(auxiliar));
      }
    }

    if (
      topCategorias.length >= 4 &&
      !topCategorias.includes(code) &&
      urlLetter == "c" &&
      index == categoryLength - 1
    ) {
      var aux = [];
      aux[0] = topCategorias[1];
      aux[1] = topCategorias[2];
      aux[2] = topCategorias[3];
      aux[3] = code;
      topCategorias = aux;
    } else if (
      !topCategorias.includes(code) &&
      urlLetter == "c" &&
      index == categoryLength - 1
    ) {
      topCategorias.push(code);
    }
    if (
      topCategorias.length >= 4 &&
      !topCategorias.includes(code) &&
      urlLetter == "p" &&
      index == categoryLength - 2
    ) {
      var aux = [];
      aux[0] = topCategorias[1];
      aux[1] = topCategorias[2];
      aux[2] = topCategorias[3];
      aux[3] = code;
      topCategorias = aux;
    } else if (
      !topCategorias.includes(code) &&
      urlLetter == "p" &&
      index == categoryLength - 2
    ) {
      topCategorias.push(code);
    }

    if (topCategorias != null && topCategorias != undefined) {
      localStorage.setItem("bdw-topC", JSON.stringify(topCategorias));
    }
  });
}

function bdwCollectP() {
  var products = [];
  var code = "";

  if (localStorage.getItem("bdw-products") != null) {
    products = JSON.parse(localStorage.getItem("bdw-products"));
  }

  $(document).on("click", ".addToCartButton", function () {
    var url = window.location.pathname.split("/");
    var urlLetter = url[url.length - 2];
    if (window.location.href == "https://www.musimundo.com/") {
      code = $(this)
        .closest(".item")
        .find(".mus-pro-desc")
        .find(".mus-pro-code")
        .html();
    } else if (urlLetter == "c") {
      code = $(this)
        .closest(".mus-product-box")
        .find(".mus-pro-desc")
        .find(".mus-pro-code")
        .html();
    } else if (urlLetter == "p") {
      code = $(".mus-pro-code").html().trim();
    }
    code = code.replace("\n\t\t\t\t\t\t\t", "");
    code = code.replace("\n\t\t\t\t\t\t", "");
    code = code.replace("\n\t\t\t\t\t", "");

    if (products.length >= 30 && !products.includes(code)) {
      var aux = [];
      for (var i = 0; i < products.length; i++) {
        if (i <= 28) {
          aux[i] = products[i + 1];
        } else if (i == 29) {
          aux[i] = code;
        }
      }
      products = aux;
    } else if (products.length < 30 && !products.includes(code)) {
      products.push(code);
    }
    if (products != null && products != undefined) {
      localStorage.setItem("bdw-products", JSON.stringify(products));
    }
  });
}

function bdwLastS() {  
    var textoBuscado = document.location.href.split('=')[1];
    if (textoBuscado != undefined) {
      fetch('https://u.braindw.com/els/musimundoapi?ft='+textoBuscado+'&qt=100&sc='+bdwGetSucCookie()+'&refreshmetadata=true&exclusive=0&aggregations=true')
      .then((resp) => resp.json())
      .then(function(data) {      
          if (data.hits != undefined) {
              console.log(textoBuscado);
              if(textoBuscado !== 'braindw&utm_medium'){
                localStorage.setItem("bdw-lastS", textoBuscado);
              }              
          }    
      })
      .catch(function(ex) {
          console.log(ex);
      });
    }
}

function bdwLastSearchClicked() {
  if (window.location.pathname == "/search/") {
    $(".span_4").on("click", function () {
      var cat = $(this).find("a:first").attr("href").split("/").pop();
      localStorage.setItem("bdw-searchClicked", cat);
    });
  } else {
    $(".ui-menu-item").on("click", function (e) {
      if ($(this).hasClass("product")) {
        var cat = $(this).find("a:first").attr("href").split("/").pop();
        localStorage.setItem("bdw-searchClicked", cat);
      }
    });
  }
}

if (isMobile && bdwConditionCategory >= 2 && bdwConfirmarClicked > 0) {
  bdwMiMusimundoMini();
} else if (bdwConditionProduct >= 2 && isMobile && bdwConfirmarClicked > 0) {
  bdwMiMusimundoMini();
}

function bdwMiMusimundoMini() {
  $("head").append(`
        <style>
            .bdw-mi-musimundo-ini a{
                display: flex;
                align-items: center;
                justify-content: space-between;
                width: 65px;
            }
        </style>
    `);
  $(".nav.clearfix").css({
    "display": "flex",
    "align-items": "flex-end",
    "justify-content": "flex-end",
  });
  if ($(".bdw-mi-musimundo-ini").length == 0) {
    $(".nav.clearfix").prepend(`
        <li class="yCmsComponent">
            <div class="bdw-mi-musimundo-ini">
                <a href="/mi-mundo">
                    <img src="https://storage.cdn.braindw.com/musimundo_hybris_produccion/script/files/2020/01/27/icomusi.png">
                    <p>Mundo</p>
                </a>
            </div>
        </li>
    `);
  }
}

function getVisitedProducts() {
  var url = window.location.pathname.split("/");
  if (url[url.length - 2] == "p") {
    var sku = $(".span_8 .mus-pro-desc .mus-pro-code").html().trim();
    if (!localStorage.getItem("bdw-productsView")) {
      localStorage.setItem("bdw-productsView", '["' + sku + '"]');
    } else {
      var allSkus = JSON.parse(
        localStorage.getItem("bdw-productsView")
      ).reverse();
      var repeated =
        allSkus.filter((item) => item == sku).length > 0 ? true : false;
      if (!repeated) {
        allSkus.push(sku);
      } else {
        allSkus.push(allSkus.splice(allSkus.indexOf(sku), 1)[0]);
      }
      if (allSkus.length > 30) {
        allSkus = allSkus.slice(allSkus.length - 30, allSkus.length);
      }
      localStorage.setItem(
        "bdw-productsView",
        JSON.stringify(allSkus.reverse())
      );
    }
  }
}

function bdwCreateExplicativeHTML() {
  $(".mus-footer").contents("#braindw_container").remove();
  try {
    var nombre = $(".logged_in").html().split(" ").pop();
    var nombreC = nombre.charAt(0).toUpperCase() + nombre.slice(1);
    document.title = $(".logged_in").html()
      ? "Musimundo de " + nombreC
      : "Mi Musimundo";
  } catch {
    document.title = "Mi Musimundo";
  }
  $("#content").contents(":not(.mus-footer)").remove();
  $("body").prepend(`
    <div class="bdw-highlight" style="position:absolute;width:100%;height:100%;background-color:black;z-index:2000;opacity:.5;">

    </div>
    <div class="bdw-explicativo-text" style="transition:all .3s;display:block;position:absolute;top:50%;left:50%;transform:translate3D(-50%,-50%,0);z-index:3000;">
    <img style="margin: 0 auto;display: block;background-color: white;border-radius: 9px;margin-bottom: 5px;padding: 5px 0;" src="https://storage.cdn.braindw.com/musimundo_hybris_produccion/script/files/2020/01/28/mimusimundodesktop.png"></img>
    <div style="border:1px solid black;width:200px;height:54px;background-color:white;text-align:center">
        <p style="line-height:17px;padding: 10px 5px;">Despues de navegar 2 categorias vas a disponer de Mi Musimundo</p>
    </div>
</div>
`);
  setTimeout(function () {
    $(".bdw-explicativo-text").css({
      opacity: "0",
    });
    setTimeout(function () {
      $(".li-nav-container").click();
    }, 300);
  }, 5000);
}
var isMobile = false;

// Buscador inteligente

if (true) {
  // Agrego el eventListener para redireccionar desde el iframe
  window.addEventListener("onmessage", (e) => {
    if (e.data && typeof e.data == "string") {
      let url = e.data;
      if (url.includes("bdwredirect|")) {
        window.location.href = url.replace("bdwredirect|", "");
      }
    }
  });
  window.addEventListener("message", (e) => {
    if (e.data && typeof e.data == "string") {
      let url = e.data;
      if (url.includes("bdwredirect|")) {
        window.location.href = url.replace("bdwredirect|", "");
      }
    }
  });

  function findGetParameter(parameterName) {
    var result = null,
      tmp = [];
    location.search
      .substr(1)
      .split("&")
      .forEach(function (item) {
        tmp = item.split("=");
        if (tmp[0] === parameterName) result = decodeURIComponent(tmp[1]);
      });
    return result;
  }
  if (window.location.href.includes("/mi-musimundo/search")) {
    const reciveFtAggs = (e) => {
      if (typeof e.data == "string" && e.data.includes("ftAggs")) {
        const url =
          "https://www.musimundo.com/mi-musimundo/search?text=" +
          findGetParameter("text") +
          "&" +
          e.data;
        window.history.pushState({ ftAggs: e.data }, "Page Title", url);
      }
    };

    window.addEventListener("popstate", () => {
      window.location.reload();
    });
    window.addEventListener("onmessage", (e) => {
      reciveFtAggs(e);
    });
    window.addEventListener("message", (e) => {
      reciveFtAggs(e);
    });

    $("#content").html("");
    const term = findGetParameter("text");
    const suc = bdwGetSucCookie();
    const ftAggs = findGetParameter("ftAggs") ? findGetParameter("ftAggs") : "";
    const iframeURL = `https://storage.cdn.braindw.com/musimundo_hybris_produccion/script/files/2020/10/05/index.html?term=${term}&suc=${suc}&ftAggs=${ftAggs}`;
    $("#content").append(
      "<iframe width='100%' id='braindw-mi-musimundo' src='" +
        iframeURL +
        "' scrolling='no' frameborder='0' width='100%'></iframe>"
    );
  }

  if (
    /(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|ipad|iris|kindle|Android|Silk|lge |maemo|midp|mmp|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows (ce|phone)|xda|xiino/i.test(
      navigator.userAgent
    ) ||
    /1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(
      navigator.userAgent.substr(0, 4)
    )
  ) {
    isMobile = true;
  }
  var sucursal = bdwGetSucCookie();
  var currSearch;

  function bdwSearch(term) {
    let customSucursal = sucursal;
    let exclusive = 0;
    if (customSucursal == "amba" || customSucursal == "ambas")
      customSucursal = "none";
    let url =
      "https://u.braindw.com/els/musimundoapi?ft=" +
      term +
      "&qt=6&sc=" +
      customSucursal +
      "&refreshmetadata=true&exclusive=" +
      exclusive;
    url = encodeURI(url);
    if (currSearch) {
      currSearch.abort();
    }
    currSearch = $.ajax({
      url: url,
      beforeSend: function () {
        var screenWidth = $(window).width() < 750 ? true : false;
        var bdwuid = screenWidth ? ".ui-widget-content" : "#ui-id-1";
        if (!screenWidth) {
          if ($(".lds-spinner").length == 0) {
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $(".control-group").append(`
                        <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-1" tabindex="0" style="display: block; width: 431px; top: 40.1719px; left: 0.5px;">
                        </ul>
                    `);

            $(bdwuid).append(`
                        <div style="margin-left:50%;transform:translateX(-20%);"><div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div></div>
                        `);
          }
        } else {
          if ($(".lds-spinner").length == 0) {
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $(".control-group").append(`
                        <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-2" tabindex="0" style="display: block; top: 36.375px; left: -0.078125px; width:115%;">
                        </ul>
                    `);

            $(bdwuid).append(`
                        <div style="margin-left:50%;transform:translateX(-20%);"><div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div></div>
                        `);
          }
        }
      },
      success: function (response) {
        var screenWidth = $(window).width() < 750 ? true : false;
        if (response != undefined && response.length > 0) {
          if (!screenWidth) {
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $(".control-group").append(`
                    <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-1" tabindex="0" style="display: block; width: 431px; top: 40.1719px; left: 0.5px;">
                    </ul>
                `);
          } else {
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $(".control-group").append(`
                    <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-2" tabindex="0" style="display: block; top: 36.375px; left: -0.078125px; width:115%;">
                    </ul>
                `);
          }

          var bdwuid = screenWidth ? ".ui-widget-content" : "#ui-id-1";
          response.slice(0, 8).map((item, index) => {
            let wordsArray = item.Descripcion.split(" ");
            let wordsText = "";
            wordsArray.slice(0, 3).map((word) => {
              wordsText += word + " ";
            });
            wordsText = wordsText.trim().replace(/ /g, "-");
            let productUTM = "?bdwsearch=true";
            /*
            let productUTM =
              "?utm_source=braindw&utm_medium=desplegable&utm_campaign=" +
              wordsText;
            */
            $(bdwuid).append(`
                        <li class="product ui-menu-item" id=${
                          "ui-id-" + (3 + index)
                        } tabindex="-1">
                            <a href=${
                              item.Link + productUTM
                            } class="product clearfix">
                            <span class="thumb">
                                <img src=${item.UrlImagen}>
                            </span>
                            <span class="desc clearfix">
                                <span class="title">${item.Descripcion}</span>
                                <span class="price">${
                                  item.PrecioInternet
                                }</span>
                            </span>
                            </a>
                        </li>
                    `);
          });
          $(".ui-autocomplete-input")
            .first()
            .on("focusout", function () {
              setTimeout(() => {
                $("#ui-id-1").remove();
                $("#ui-id-2").remove();
              }, 200);
            });
          $(".ui-autocomplete-input")
            .last()
            .on("focusout", function () {
              setTimeout(() => {
                $("#ui-id-1").remove();
                $("#ui-id-2").remove();
              }, 200);
            });
        } else {
          if (!screenWidth) {
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $("#ui-id-1").remove();
            $(".control-group").append(`
                    <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-1" tabindex="0" style="display: block; width: 431px; top: 40.1719px; left: 0.5px;">
                        <li id="3" tabindex="-1">No se encontraron productos</li>
                    </ul>
                `);
          } else {
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $("#ui-id-2").remove();
            $(".control-group").append(`
                    <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-2" tabindex="0" style="display: block; top: 36.375px; left: -0.078125px; width:115%;">
                        <li id="3" tabindex="-1">No se encontraron productos</li>
                    </ul>
                `);
          }

          $(".ui-autocomplete-input")
            .first()
            .on("focusout", function () {
              $("#ui-id-1").remove();
              $("#ui-id-2").remove();
            });
          $(".ui-autocomplete-input")
            .last()
            .on("focusout", function () {
              $("#ui-id-1").remove();
              $("#ui-id-2").remove();
            });
        }
      },
    });
  }

