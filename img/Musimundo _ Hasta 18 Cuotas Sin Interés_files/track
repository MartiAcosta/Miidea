  function splitString(str) {
    var middle = Math.ceil(str.length / 2);
    var s1 = str.slice(0, middle);
    var s2 = str.slice(middle);
    var response = s1 == s2 ? s1 : str;
    return response;
  }
  $("head")["append"](
    "\x0A    <style>\x0A    .searchmobileico {\x0A        background-size: 27px 27px!important;\x0A        right: 0!important;\x0A    }\x0A    .searchbyvoice_mic_on {\x0A        position: absolute;\x0A        right: 50px;\x0A        top: 7px;\x0A        background: url(https://storage.cdn.braindw.com/braindw_developer/script/files/2020/07/03/microfonogrisremovebgpreview.png) no-repeat center;\x0A        background-size: 21px 21px;\x0A        border: none;\x0A        cursor: pointer;\x0A        margin-inline-end: 16px;\x0A        padding: 22px 12px 0;\x0A        width: 21px;\x0A        background-color: transparent!important;\x0A        border: none!important;\x0A    }\x0A    .microphone-icon-rec {\x0A        position: absolute;\x0A        right: 50px;\x0A        top: 7px;\x0A        background: url(https://storage.cdn.braindw.com/braindw_developer/script/files/2020/07/03/microfonorojoremovebgpreview.png) no-repeat center;\x0A        background-size: 21px 21px;\x0A        border: none;\x0A        cursor: pointer;\x0A        margin-inline-end: 16px;\x0A        padding: 22px 12px 0;\x0A        width: 21px;\x0A        background-color: transparent!important;\x0A        border: none!important;\x0A    }\x0A    </style>\x0A    "
  ),
    isMobile
      ? $(".simple_disp-img")["append"](
          '\x0A            <button class="search-mic searchmobileico searchbyvoice-trigger searchbyvoice_mic_on" title="Buscar por voz"></button>\x0A        '
        )
      : $(".controls")["prepend"](
          '\x0A        <button type="button" class="search-mic searchbyvoice-trigger searchbyvoice_mic_on microphone-icon" title="Buscar por voz"></button>\x0A    '
        ),
    (window["searchbyvoice_search_id"] = "busca-v"),
    (window["searchbyvoice_voice_language"] = "es-419"),
    $(".siteSearchInput.input_SearchBox")["attr"](
      "id",
      window["searchbyvoice_search_id"]
    ),
