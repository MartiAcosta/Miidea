        var _bdwSettings = BdwBuildConfigObject(window[_braindw_gondola + "_resultJson"].id, window[_braindw_gondola + "_resultJson"].settings);
        Gondola_template = BraindwRenderBannerDynamic(bdwJsonResult, _braindw_gondola, braindw_sin_title, braindw_sin_Events, braindw_sin_title1, braindw_sin_title2, braindw_sin_utmquery, _bdwSettings)
    } else {
        var linkImgBdw = '';
        try {
            var objbdw = window[_braindw_gondola + "_resultJson"].settings;
            for (var i = 0; i < objbdw.length; i++) {
                if (objbdw[i].key == 'url')
                    linkImgBdw = objbdw[i].value;
                else if (objbdw[i].key == 'title')
                    braindw_sin_title = objbdw[i].value;
                else if (objbdw[i].key == 'title1')
                    braindw_sin_title1 = objbdw[i].value;
                else if (objbdw[i].key == 'title2')
                    braindw_sin_title2 = objbdw[i].value;
            }
        } catch (e) {}
        var Gondola_template = BraindwRenderBannerDynamic(bdwJsonResult, _braindw_gondola, braindw_sin_title, braindw_sin_Events, braindw_sin_title1, braindw_sin_title2, braindw_sin_utmquery, null, linkImgBdw);
    }
    switch ("after") {
        case "append":
            $(".braindw-gondola-mejorestv-home").first().append(Gondola_template);
            break;
        case "prepend":
            $(".braindw-gondola-mejorestv-home").first().prepend(Gondola_template);
            break;
        case "after":
            $(".braindw-gondola-mejorestv-home").first().after(Gondola_template);
            break;
        case "before":
            $(".braindw-gondola-mejorestv-home").first().before(Gondola_template);
            break
    }
    if (typeof window['braindw_2' + "_resultJson"] != "undefined" && bdwMobile.any() == null) {
        braindwStartCarousel('braindw_2', _bdwSettings.carouselItemsNum, _bdwSettings.carouselItemsMargin);
    } else {
        braindwStartCarousel('braindw_2', 4, 10);
    }
    BraindwLoadEvent();
    if (location.href.match(/.*?:\/\/(.*).musimundo.com\/cart(.*)$/g) != null) {
        $('#braindw_container .mus-ribbon.right').css('padding','12px 12px');
        $('.cuotas_nro').css('font-size', '24px');
        $('.cuotas_texto_1').css('font-size', '10px');
        $('.cuotas_texto_2').css('font-size','7px');
    }
} catch (e) {
    console.log(e)
}$("body").append(`<script>
        var _bdwMiMusiCount =0;
        var restarted = false;
        window.onmessage = function(event) {
            if(event.data == "bdw-restart" && !restarted){
                restarted = true;
                bdwCreateHTML();
            }
            if(event.data == "bdw-count-m"){
                _bdwMiMusiCount +=1;
