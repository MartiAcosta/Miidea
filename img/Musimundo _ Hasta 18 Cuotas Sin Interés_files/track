}

function BraindwRenderBannerDynamic(data, braindw_gondola, braindw_title, braindw_Events, braindw_title_small, braindw_title_small1,utmquery,configs,linkImgBdw) {

    var divsImagesAppend = '';
    var titleConfigured = '';
    var displayTitulo = 'block';
    var linkViewMore = '';
    var navigationContainerWidth='';
    var itemStyle ='';
    var carouselItemsNum = 4;

    var IsCart = window.location.href.match((/.*?:\/\/(.*).musimundo.com\/cart(.*)$/)) !== null;
    if(IsCart && bdwMobile.any() == null)
    {
        carouselItemsNum = 3;
    }

    if(configs != undefined && configs != null){
  
        for(var i = 0; i < configs.imagesObjects.length ; i++){
            var img = configs.imagesObjects[i];
            divsImagesAppend += BdwBuildImageObject(img.url, img.linkImg, img.width, img.marginSide, img.floatImage, img.margin_top);
            divsImagesAppend = divsImagesAppend.replace(/%eventoimg%/g,' bw-title="' + braindw_gondola +'" bw-code="" bw-section="' + braindw_Events +'" bw_section="' + braindw_Events +'"' );
            if (linkViewMore=='')
                linkViewMore = img.linkImg;
        }  
        if ((configs.titleLeft.value == '' && configs.titleRight.value == '' && configs.title.value=='') || (configs.titleLeft == null && configs.titleRight == null && configs.title.value==null))
             displayTitulo = 'none';

        titleConfigured = '<div class="mus-heading" style="display:' + displayTitulo + ';"><h2>' + configs.title.value + '</h2><span class="mus-heading-lead">' + configs.titleLeft.value + ' <br> ' + configs.titleRight.value + '</span></div>';
        itemStyle = configs.itemStyle;
        navigationContainerWidth=configs.navigationContainerWidth;
        carouselItemsNum =configs.carouselItemsNum;             
    }
    else if (braindw_title.trim() != ''){
        titleConfigured = '<div class="mus-heading" ><h2 class="_braindw_slider_title _braindw_slider_title_mob">' + braindw_title + '</h2><span class="mus-heading-lead">' + braindw_title_small+ ' <br> ' + braindw_title_small1 + '</span></div>';
    }
    if (linkViewMore=='' && linkImgBdw!='')
        linkViewMore=linkImgBdw;
    
    if (data.length > 0) {
      var disp="";
    /*  if(braindw_title.toUpperCase() =="#LOS MAS VENDIDOS DE LA CATEGORÍA"){
          var disp="none";
      }*/
      var template = '<section id="braindw_container" style="display: '+ disp +'">'+
              '<div class="section-bdw" style="height:700px;" >'+
              '<div class="braindw-heading" >'+
              '<h2>'+braindw_title+'</h2>'+
              '<span class="braindw-heading-lead">'+braindw_title_small+
              ' <br> ' + braindw_title_small1 +'</span></div>'+
              '<div class="braindw_row">'+            
              divsImagesAppend+
              '<div class="braindw_columns">'+
              '<div class="braindw_owl-carousel braindw_owl-theme '+braindw_gondola+'">';
        var itemTemplate = '<div class="braindw_item">'+
                "<div  class='mus-product-box clearfix productGridItem red-box ' >" +
                "<div class='mus-pro-icon '>" +
                " <div class='aire'></div>" +
                "</div> " +
                "<a href='%link%' class='mus-product-box clearfix productGridItem red-box _braindwTriggerClick mus-pro-thumb' bw-code='%code%' bw-title='%Descripcion%' bw_section='"+braindw_Events+"'  >" +
                "<img src='%urlImage%' alt='%title%' title='%title%'/ >" +
                "</a>" +
                "<a href='%link%' class='mus-product-box clearfix productGridItem red-box desc-box-holder _braindwTriggerClick' bw-code='%code%' bw-title='%Descripcion%' bw_section='"+braindw_Events+"' >" +
                " <div class='mus-pro-brand' style='color: #9e9e9e;font-size: 12px;text-align: center;font-weight: 400;height: 25px;'>" +
                "  <span>%brand%</span>" +
                " </div>" +
                " <div class='mus-pro-desc'>" +
                "  <p class='mus-pro-name strong' style='margin-bottom: 4px;font-size: 1.2em;' >%title%</p>" +
                "  <p class='mus-pro-code'>%code%</p>" +
                "  <p class='mus-pro-rate'></p>" +
                " </div>" +
                "</a>" +
                "<div class='price-box-holder' style='visibility: visible'>" +
                "<div class='mus-pro-price-box'>" +
                "<p class='mus-pro-price-cash'>" +
                " <span class='mus-pro-price-label' style='text-transform: none;'>%precio_online%</span>" +
                "  <span class='mus-pro-price-save' style='text-decoration: line-through;'>%priceOld%</span>" +
                " </p>" +
                " <p class='mus-pro-price-saving' style='visibility: %visibility_ahorra%;background-color:#ff4338;'>" +
                "  <span class='mus-pro-price-label' style='padding-left: 5px;padding-right: 2px;color:white;text-transform: none;'>Ahorro: </span>" +
                "  <span class='mus-pro-price-price' style='background-color: #ff4338;font-weight: bold;font-size: 15px;position: relative;-webkit-border-radius: 30px;-moz-border-radius: 30px;border-radius: 30px;padding: 0px 0px 0px 0px;color:white;top: 2px;'>$ %priceBonification%%priceBonificationDecimal%</span>" +
                " </p>" +
                " <p class='mus-pro-price' style='padding-bottom:20px;'>" +
                "  <span class='mus-pro-price-label strong' style='float:left;text-transform: none;padding-right: 5px;'>Precio Online: </span>" +
                "  <span class='mus-pro-price-number' style='float:left;'>" +
                "   <span>%priceOnline%</span>" +
                "  </span>" +
                " </p>   " +
                " </div>" +
                " </div>" +
                "  <div class='mus-pro-actions braindw_button' style='margin-top:-60px;'>" +
                "   <input type='hidden' name='productCodePost' value='%code%'/>" +
                "   <input type='hidden' name='productNamePost' value='%title%'/>" +
                "   <input type='hidden' name='productPostPrice' value='%priceOnline%'/>" +
                //DIV AGREGADO PRECIO CONTADO SIN CUOTA//
                '<div class="mus-pro-quotes" style="height:35px;max-height:35px;text-align:center;font-weight:700;display:%displaysinquotas%">%priceOnline%</div>' +
                //FIN DIV AGREGADO//
                '<div class="mus-pro-quotes" style="display:%cuotasSinInteres1%;margin-left:8px;height:35px;max-height:35px;overflow:hidden;text-align:center;"> %CantidadCuotas% x <span class="mus-pro-quotes-price strong">%cuota%</span></div>' +
                "<a href='%link%' class='_braindwTriggerClick' bw-title='%title%' bw-code='%code%' bw_section='"+braindw_Events+"'>" +
                '<button id="btn-stash"  aria-disabled="true" class="addToCartButton brandw_button" style="font-family: futura-bold;min-width: 195px;font-size: 15px;width: auto;color: #fff;"> Ver más</button>' +
                "<button class='btn-primary hidden'>Comprar en un paso</button>" +
                "</a>" +
                "  </div>" +
                "  <div>" +
                "   <input type='hidden' name='CSRFToken' value='cce3c932-3878-495f-ab13-5698d5e5090f' />" +
                "  </div>" +
                "<div class='mus-ribbon saving-ribbon' style='visibility:%visibility_ahorra%;left:15px; background-image: url(https://www.musimundo.com/_ui/desktop/theme-musimundo/images/common/promos/placa_ahorro.png); top:-11px;background-color:transparent'>" +
                "<div><span class='ahorro' style='background-size: cover;font-family: futura-bold;text-transform: uppercase;'> " +
                "Ahorro</span></div>" +
                "<div> " +
                "<span class='precio'> " +
                "$<em>%priceBonification%</em>%priceBonificationDecimal%</span>" +
                "</div> " +
                "<div>" +
                "</div> " +
                "</div> " +
                "<br>" +
                "<div class='mus-ribbon options-ribbon right' style='display:%cuotasSinInteres%;'>" +
                "<div><span class='cuotas_nro' style='background-size: cover;font-family: futura-bold;text-transform: uppercase;float:left;display: block;padding-right:3px;margin-bottom: 9px;'>%CantidadCuotas%</span></div>" +
                "<div style='float: right;margin-top: -6px;'>" +
                "<span class='cuotas_texto_1' style='font-size: 12px;float: none;letter-spacing: 0.05em;line-height: 10px;min-width: 30px;font-weight: 900;'" +
                "text-align: center;font-family: Roboto, sans-serif;margin-top: 5px;display: block;'>CUOTAS</span>" +
                "<span class='cuotas_texto_2' style='font-size: 8px;float: left;letter-spacing: 0.05em;line-height: 0px;min-width: 30px;" +
                "font-weight: 400;text-align: center;font-family: Roboto, sans-serif;display: block;margin-top: 6px;'>SIN INTERÉS</span>" +
                "</div>" +
                "</div>" +
                "</div>" +
                '</div>';

            var index = 0;
            var totalRow = 0;    

            $.each(data, function(i, item) {
            index = index + 1;
            totalRow = totalRow + 1;    
            var customLink = item.Link;
            if (utmquery != undefined && utmquery != '') {
                if (item.Link.indexOf('?') == -1) customLink = customLink + '?';
                customLink = customLink + utmquery;
            }
            var customItem = itemTemplate.replace("%brand%", item.Marca);


            var braindw_CucardaOferta = item.CucardaOferta;
            var braindw_CucardaOfertaDeciaml = '';
            if (item.CucardaOferta != null && item.CucardaOferta.toLowerCase() != 'null') {
                braindw_CucardaOferta = item.CucardaOferta;
                customItem = customItem.replace(/%visibility_ahorra%/g, 'visible');
                braindw_CucardaOferta = braindw_CucardaOferta.replace('$ ', '');

                if (braindw_CucardaOferta.indexOf(',')) {
                    braindw_CucardaOfertaDeciaml = ',' + braindw_CucardaOferta.split(',')[1];
                    braindw_CucardaOferta = braindw_CucardaOferta.split(',')[0];
                }
            } else if (braindw_CucardaOferta == null || braindw_CucardaOferta == 'null') {
                customItem = customItem.replace(/%visibility_ahorra%/g, 'hidden');
                braindw_CucardaOferta = '';
            }

            var braindw_PrecioInternet = item.PrecioInternet;
            if (item.PrecioInternet != null && item.PrecioInternet.toLowerCase() != 'null') {
                braindw_PrecioInternet = item.PrecioInternet;
                if (braindw_PrecioInternet.indexOf("$") == -1)
                    braindw_PrecioInternet = "$ " + braindw_PrecioInternet;
            } else if (braindw_PrecioInternet == null || braindw_PrecioInternet == 'null') {
                braindw_PrecioInternet = '';
            }
             var braindw_PrecioAnterior = item.PrecioAnterior;
            if (item.PrecioAnterior != null && braindw_PrecioAnterior.toLowerCase() != 'null') {
                braindw_PrecioAnterior = item.PrecioAnterior;
                if (braindw_PrecioAnterior.indexOf("$") == -1)
                    braindw_PrecioAnterior = "$ " + braindw_PrecioAnterior;

                customItem = customItem.replace(/%precio_online%/g, 'Precio Contado:');
            } else if (braindw_PrecioAnterior == null || braindw_PrecioAnterior.toLowerCase() == 'null') {
                braindw_PrecioAnterior = '';
                customItem = customItem.replace(/%precio_online%/g, '');
            }

            customItem = customItem.replace(/%Descripcion%/g, item.Descripcion);
            customItem = customItem.replace(/%urlImage%/g, item.UrlImagen);
            customItem = customItem.replace(/%link%/g, customLink);
            customItem = customItem.replace(/%title%/g, item.Descripcion.replace("\"", ""));
            customItem = customItem.replace(/%code%/g, item.IdArticulo);
            customItem = customItem.replace(/%priceOld%/g, braindw_PrecioAnterior);
            customItem = customItem.replace(/%priceOnline%/g, '$' + braindw_PrecioInternet);
            customItem = customItem.replace(/%priceBonificationDecimal%/g, braindw_CucardaOfertaDeciaml);
            customItem = customItem.replace(/%priceBonification%/g, braindw_CucardaOferta);  

            try {
                var idHybris = item.UrlImagen.split('/')[4].split('-')[0];
                customItem = customItem.replace(/%idHybris%/g, idHybris);
            } catch (e) { }          
       

             if (item.Cuota_Numero != '' && item.Cuota_Numero != '0' && item.Cuota_Numero != '1' && item.Cuota_Numero != null) {
                customItem = customItem.replace(/%cuotasSinInteres%/g, 'block');
                customItem = customItem.replace(/%cuotasSinInteres1%/g, 'block');
                customItem = customItem.replace(/%displaysinquotas%/g, 'none'); 
            }
            else {
                customItem = customItem.replace(/%cuotasSinInteres%/g, 'none');
                customItem = customItem.replace(/%displaysinquotas%/g, 'block');                
            }

            if (item.Cuota_Numero == '1') {
                customItem = customItem.replace(/%cuotasSinInteres1%/g, 'none');
                customItem = customItem.replace('%CantidadCuotas% x <span class="mus-pro-quotes-price strong">%cuota%</span>', '<span class="mus-pro-quotes-price strong">%cuota%</span>');
            } else {
                customItem = customItem.replace(/%cuotasSinInteres1%/g, 'none');
                customItem = customItem.replace(/%displaysinquotas%/g, 'block');
            }
            customItem = customItem.replace(/%displaysinquotas%/g, 'none'); 
            customItem = customItem.replace(/%cuota%/g, item.Cuota_Monto);
            customItem = customItem.replace(/%CantidadCuotas%/g, item.Cuota_Numero);
            template = template + customItem;

        });        
        template = template + 
            '</div>'+
            '</div>'+
          '</div>'+
          '</div>'+
        '</section>%divButtonViewMore%</div>';
            
        if (linkViewMore!='' && bdwMobile.any()){
            template = template.replace('%divButtonViewMore%', '<a class="color bwd_viewmore _braindwTriggerClick" bw-title="ver mas" bw-code="" bw_section="'+braindw_Events+'" bw-section="%sectionEvent%" href="' + linkViewMore + '" target="_top"><div class="seemore">VER MÁS</div></a>');
            template = template.replace(/%sectionEvent%/g, braindw_Events);
        }
        else
            template = template.replace('%divButtonViewMore%','');

        try{
            if(configs != undefined && configs != null)
                template += GondolaStyleDynamic(braindw_gondola, configs.sliderWidth, configs.sliderFloatSide);
            else
                template += GondolaStyleDynamic(braindw_gondola, '100%', 'initial');
        }
        catch(e){
            console.log(e);
        }
        return template;
    }
}

function BdwBuildImageObject(urlImage, linkImage, widthImage, marginSideImage, floatImage, marginTop){
    return '<div class="_braindw_item_image _braindwTriggerClick" style="width: '+widthImage+';'+marginSideImage+';float: '+floatImage+';"  %eventoimg% ><a href="'+linkImage+'"><img src="'+urlImage+'" style="margin-top:'+marginTop+'"></a></div>';
}

function BdwBuildConfigObject(idTemplateConfig, paramsConfig){
    var bdwTemplateConfig = {};
    
    var differentGroups = [];
    for(var i=0; i<paramsConfig.length; i++){
        var keysConfig = Object.entries(paramsConfig[i]);
        for(var j=0; j<keysConfig.length; j++){
            if(keysConfig[j][0] == "group" && !differentGroups.includes(keysConfig[j][1])){
                if(keysConfig[j][1] != "undefined"){
                    differentGroups.push(keysConfig[j][1]);
                }
            }
                
        }
    }
    
    var valuesImageGeneral = {};
    switch(idTemplateConfig){
        case '0001':
        {
            bdwTemplateConfig = {
                sliderWidth: '73%',
                navigationContainerWidth: 'width: 73%;float: right;margin-top: -12px;',
                sliderFloatSide: 'right',
                navArrowMargin: 'margin-right: 0',
                carouselItemsNum: 3,
                carouselItemsMargin: 18,
                itemStyle: "30% !important;margin-left:24px;"
            };
            valuesImageGeneral = {
                width: '305px',
                margin_top: '8%',
                marginSide: ['padding:0 0 0 20px;'],
                floatImage: ['left']
            };
            break;
        }   
        case '0002':
        {
            bdwTemplateConfig = {
                sliderWidth: '73%',
                navigationContainerWidth: 'width: 73%;float: left;margin-top: -12px;',
                sliderFloatSide: 'right',
                navArrowMargin: 'margin-right: 0',
                carouselItemsNum: 3,
                carouselItemsMargin: 18,
                itemStyle: "30% !important;margin-left:22px;"
            };
            valuesImageGeneral = {
                width: '305px',
                margin_top: '26%',
                marginSide: ['width: 305px;padding:0 20px 0 0;float: right;'],
                floatImage: ['right']
            };
            break;
        }
        case '0003':
        {
            bdwTemplateConfig = {
                sliderWidth: '48%',
                navigationContainerWidth: 'width: 48%;float: left;margin-top: -12px;',
                sliderFloatSide: 'right',
                navArrowMargin: 'margin-right: 0',
                carouselItemsNum: 2,
                carouselItemsMargin: 18,
                itemStyle: "45% !important;margin-left:20px;"
            };
            valuesImageGeneral = {
                width: '305px',
                margin_top: '26%',
                marginSide: ['padding: 0 0 0 20px;', 'padding: 0 20px 0 0;'],
                floatImage: ['left','right']
            };
            break;
        }
        case '0004':
        {
            bdwTemplateConfig = {
                sliderWidth: '49%',
                navigationContainerWidth: 'width: 49%;float: left;margin-top: -12px;',
                sliderFloatSide: 'right',
                navArrowMargin: 'margin-right: 0',
                carouselItemsNum: 2,
                carouselItemsMargin: 18,
                itemStyle: "44% !important;margin-left:24px;"
            };
            valuesImageGeneral = {
                width: '610px',
                margin_top: '13%', 
                marginSide: ['padding: 0 0 0 20px;'],
                floatImage: ['left']
            };
            break;
        }
        case '0005':
        {
            bdwTemplateConfig = {
                sliderWidth: '48%',
                navigationContainerWidth: 'width: 49%;float: left;margin-top: -12px;',
                sliderFloatSide: 'right',
                navArrowMargin: 'margin-right: 0',
                carouselItemsNum: 2,
                carouselItemsMargin: 18,
                itemStyle: "44% !important;margin-left:24px;"
            };
            valuesImageGeneral = {
                width: '305px',
                margin_top: '26%',
                marginSide: ['padding: 0 0 0 20px;', 'padding: 0 0 0 20px;'],
                floatImage: ['left','left']
            };
            break;
        }
    }
    
    var imagesObjects = [];
    for(var k=0; k<differentGroups.length; k++){
        var group = differentGroups[k];
        var linkData = paramsConfig.filter(function(value){
            return value.group == group && value.type == "link";
        });
        var imgUrlData = paramsConfig.filter(function(value){
            return value.group == group && value.type == "img";
        });
        imagesObjects.push({
            url: imgUrlData[0].value,
            linkImg: linkData[0].value,
            width: valuesImageGeneral.width,
            floatImage: valuesImageGeneral.floatImage[k],
            marginSide: valuesImageGeneral.marginSide[k],
            margin_top: valuesImageGeneral.margin_top
        });
    }    
    bdwTemplateConfig.imagesObjects = imagesObjects;    
    bdwTemplateConfig.title = paramsConfig.filter(function(value){
        return value.key == "title";
    });   
    bdwTemplateConfig.title = bdwTemplateConfig.title != null ? bdwTemplateConfig.title[0] : null;   
    bdwTemplateConfig.titleLeft = paramsConfig.filter(function(value){
        return value.key == "title1";
    });   
    bdwTemplateConfig.titleLeft = bdwTemplateConfig.titleLeft != null ? bdwTemplateConfig.titleLeft[0] : null;
    
    bdwTemplateConfig.titleRight = paramsConfig.filter(function(value){
        return value.key == "title2";
    });   
    bdwTemplateConfig.titleRight = bdwTemplateConfig.titleRight != null ? bdwTemplateConfig.titleRight[0] : null;   
    return bdwTemplateConfig;
}

var bdwMobile = {
 Android: function() {
   return navigator.userAgent.match(/Android/i);
 },
 BlackBerry: function() {
   return navigator.userAgent.match(/BlackBerry/i);
 },
 iOS: function() {
   return navigator.userAgent.match(/iPhone|iPad|iPod/i);
 },
 Opera: function() {
   return navigator.userAgent.match(/Opera Mini/i);
 },
 Windows: function() {
   return navigator.userAgent.match(/IEMobile/i);
 },
 any: function() {
   return (bdwMobile.Android() || bdwMobile.BlackBerry() || bdwMobile.iOS() || bdwMobile.Opera() || bdwMobile.Windows());
 }
};

function braindwStartCarousel(gondola, items, margin){
  
    var IsCart = window.location.href.match((/.*?:\/\/(.*).musimundo.com\/cart(.*)$/)) !== null;
    var isMobile = window.matchMedia("(max-width: 430px)");
    if(IsCart && bdwMobile.any() == null)
    {
        items = 3;
    }

    if (isMobile.matches) {
        $('.' + gondola).owlCarousel({
          margin: margin,
          nav: true,
          dots: false,
          loop: false,
          rewind: true,
          items: items,
          paginationSpeed: 400,
          autoplay: false,
          autoplayHoverPause: false,
          responsiveClass:true,
          responsive : {
            0:{
              items:1,
              },
            1000: {
                  items: items,
                  }
          }
        });
    } else {
        $('.' + gondola).owlCarousel({
            margin: margin,
            nav: true,
            dots: false,
            loop: false,
            rewind: true,
            items: items,
            paginationSpeed: 400,
            autoplay: 4000,
            autoplayHoverPause: true,
            responsiveClass:true,
            responsive : {
              0:{
                items:1,
            },
              1000: {
                    items: items,
                }
            }
        });    
    }  
