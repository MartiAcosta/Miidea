try {

    if(braindw_sin_title === '#LOS MAS VISTOS' || braindw_sin_title === ''){
            braindw_sin_title = braindw_sin_title3;
    }

    if (bdwMobile.any() && bdwJsonResult.length >= 6) {
        bdwJsonResult = bdwJsonResult.splice(0, 6);
    } else {
        var ResultMod = bdwJsonResult.length % 4;
        if (ResultMod != 0) {
            if (_braindw_gondola != "gondola_ultimos_navegados") {
                bdwJsonResult.splice(bdwJsonResult.length - ResultMod, ResultMod);
            } else {
                if (bdwJsonResult.length < 2)
                    bdwJsonResult.splice(bdwJsonResult.length - ResultMod, ResultMod);
            }
        }
    }
    if (typeof window[_braindw_gondola + "_resultJson"] != "undefined" && bdwMobile.any() == null) {
