  $("body").append(`<div id="bdw-productos-visitados"></div>`);
  $("body").append(`<div id="bdw-categorias-visitadas"></div>`);
  */
  let bdwIsMobile = window.innerWidth > 420 ? false : true;
  let bdwGondolasAmount = 0;
  let bdwInitialDivs = [
    {
      id: "bdw-ultima-busqueda",
      localStorage: "bdw-lastS",
    },
    {
      id: "bdw-categorias-navegadas",
      localStorage: "bdw-topC",
    },
    {
      id: "bdw-categorias-visitadas",
      localStorage: "bdw-categorias",
