                    //scrollButtons: {
                    //    enable: true,
                    //    scrollAmount: "auto"
                    //},
                    theme: "dark-thin",
                    mouseWheel: { preventDefault: true },
                    callbacks: {
                        onUpdate: function () {
                            if (!$o.chat.noUpdateScroll) {
                                $("#oct8ne-chat-room").mCustomScrollbar("scrollTo", "bottom", { callbacks: false });
                            }
                            //else {
                            //    $o.chat.noUpdateScroll = false;
