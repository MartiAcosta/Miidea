                            }
                        }
                    },
                    alwaysShowScrollbar: 2
                });
            } else {
                document.getElementById("server-overload").style.display = 'block';
            }
        },


        disableScroll: function () { //Muestra Boton y descativa scroll temporalmente
            $("#go-to-bottom").fadeIn(500);
